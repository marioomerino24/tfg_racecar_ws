<launch>
  <arg name="config" default="$(find racecar_pure_pursuit_control)/config/pure_pursuit_control.yaml"/>
  <rosparam command="load" file="$(arg config)"/>

  <node pkg="racecar_pure_pursuit_control"
        type="pure_pursuit_controllers.py"
        name="pure_pursuit_controller"
        output="screen"/>
</launch>
